package cloning;
class Sum {
	int num1, num2;
	void getSum() {
		System.out.println("Sum="+(num1+num2));
	}
}
public class CloneMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
           Sum s1 = new Sum();
           s1.num1=10;
           s1.num2=20;
           s1.getSum();
           Sum s3 =new Sum();
           s3.num1=s1.num1;
           s3.num2=s1.num2;
           s3.getSum();   
//           Sum s2=s1;  //Shadow Copy
//           s2.num1=40;
//           s2.getSum();
//           s1.getSum();
	}

}
