package interfacepkg;

interface Rectangle {
	void getArea();
}
interface Circle {
	void getArea();
}
interface Area extends Rectangle, Circle{
	void getArea();
}

class Shape implements Rectangle,Circle {

	int l,b;
	int r;
	Shape(int l, int b){
		this.l=l;
		this.b=b;
	}
	Shape(int r){
		this.r=r;
	}
	
	@Override
	public void getArea() {
		// TODO Auto-generated method stub
		
	}
		
}
public class AreaMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
            Rectangle s1= new Shape(2,5);
            s1.getArea();
            Circle s2=new Shape(23);
            s2.getArea();
	}

}
